{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "defaultProject": "suo-website",
  "generators": {
    "@nrwl/gatsby": {
      "application": {}
    }
  },
  "projects": {
    "suo-website": {
      "root": "apps/suo-website",
      "sourceRoot": "apps/suo-website/src",
      "projectType": "application",
      "generators": {},
      "targets": {
        "build": {
          "executor": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/suo-website/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "suo-website:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "suo-website:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/suo-website/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/suo-website"
          ],
          "options": {
            "jestConfig": "apps/suo-website/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "suo-website-e2e": {
      "root": "apps/suo-website-e2e",
      "sourceRoot": "apps/suo-website-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/suo-website-e2e/cypress.json",
            "tsConfig": "apps/suo-website-e2e/tsconfig.e2e.json",
            "devServerTarget": "suo-website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "suo-website:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/suo-website-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "shared-designtokens": {
      "root": "libs/shared/designtokens",
      "sourceRoot": "libs/shared/designtokens/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/designtokens/**/*.js"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/designtokens"
          ],
          "options": {
            "jestConfig": "libs/shared/designtokens/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-styles": {
      "root": "libs/shared/styles",
      "sourceRoot": "libs/shared/styles/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/styles/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/styles"
          ],
          "options": {
            "jestConfig": "libs/shared/styles/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui": {
      "root": "libs/shared/ui",
      "sourceRoot": "libs/shared/ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/ui"
          ],
          "options": {
            "jestConfig": "libs/shared/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/shared/ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/shared-ui",
            "config": {
              "configFolder": "libs/shared/ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "shared-storybook-decorators": {
      "root": "libs/shared/storybook-decorators",
      "sourceRoot": "libs/shared/storybook-decorators/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/storybook-decorators/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/storybook-decorators"
          ],
          "options": {
            "jestConfig": "libs/shared/storybook-decorators/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}